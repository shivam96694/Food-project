<form action="/food/display_by_id" >
    <div class="maincontainer">
        <div class="box shadow p-3 mb-5 bg-white rounded ">
            <div class="container">
            <div class="row">
                <div class="col-sm-12 ">
<div class="titlediv">
             <img src="/images/burger.png" class="imagesize" />
                <div class="heading">  Food Register</div>     
</div>

                     <div style="display: flex;flex-direction:column;width:167px;background:red;margin-bottom:10px">
                            <div class="divider"></div>
                             <div style="color:#fff; align-self:center; font-size: 12px;font-weight:500;letter-spacing:1px;">Edit Food Details</div>
                       </div>

</div>
</div>
<div class="row gap" >
    <div class="col-sm-12">
       <input required type="text" name="fooddataid" class="form-control"  placeholder="Fill Food Id">
    </div>

    <div class="row" style="margin-top:10px;">
    <div class="col-sm-6" style="display:flex;justify-content:center">
       <button type="submit" class="btn btn-outline-success" style="height: 40px;width:50%">Submit</button>
    </div>
    <div class="col-sm-6" style="display:flex;justify-content:center">
       <button type="reset" class="btn btn-outline-danger" style="height: 40px;width:50%">Reset</button>
    </div>
 </div>

 <div class="row" style="margin-top:10px;">
      <div class="col-sm-6" style="display:flex;justify-content:center">
         <b><%=message%></b>
    </div>
       </div>

       
            
</div>
 </div>

 </div>
